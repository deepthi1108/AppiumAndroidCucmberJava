import org.openqa.selenium.WebElement;


import utility.productsDisplay;

public class productPage {
	WebDriver driver;
	WebElement element;

	 public void validateProductPage(String option) {
		driver.findElement(By.id("productsDisplay."+option ));
		
		}

}
